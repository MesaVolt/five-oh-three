{
    "name": "mesavolt/five-oh-three",
    "description": "Maintenance page, pretty and simple.",
    "keywords": ["503", "maintenance"],
    "license": "MIT",
    "authors": [
        {
            "name": "MesaVolt",
            "email": "contact@mesavolt.com"
        }
    ],
    "type": "library",
    "config": {
        "sort-packages": true
